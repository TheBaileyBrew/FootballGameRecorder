                            android:id="@+id/qb_one_pass_attempt"
                            android:onClick="onClickTeamOnePassComplete"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/team_one_pass_stats"
                            android:text="complete"
                            android:textSize="@dimen/small_font"
                            />
                        <Button
