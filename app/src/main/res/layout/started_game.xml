                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/team_one_rec_stats"
                            android:layout_toRightOf="@id/team_one_receiving_plus_one"
                            android:text="+10"
                            android:textSize="@dimen/small_font"
                            />
