                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/small_med_padding"
                            android:text="COMPLETED: "/>
                        <TextView
                            android:id="@+id/teamOne_completed"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textStyle="bold"
                            android:text="0"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/small_med_padding"
                            android:text="INTS: "/>
                        <TextView
                            android:id="@+id/teamOne_interceptions"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textStyle="bold"
                            android:text="0"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/small_med_padding"
                            android:text="FUMBLES: "/>
                        <TextView
                            android:id="@+id/teamOne_quarterback_fumble"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textStyle="bold"
                            android:text="0"/>
                    </LinearLayout>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="STARTING RUNNINGBACK STATISTICS"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/medium_padding">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="TOUCHDOWNS: "/>
                        <TextView
                            android:id="@+id/rushing_td"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textStyle="bold"
                            android:text="0"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/small_med_padding"
                            android:text="FUMBLES: "/>
                        <TextView
                            android:id="@+id/teamOne_runningback_fumble"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textStyle="bold"
                            android:text="0"/>
                    </LinearLayout>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="STARTING WIDE RECEIVER STATISTICS"/>
